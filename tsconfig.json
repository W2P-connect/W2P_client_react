{
    "compilerOptions": {
        "target": "es6", // ou une autre version selon vos besoins
        "module": "commonjs",
        "jsx": "react-jsx",
        "checkJs": false,
        "strict": true,
        "allowJs": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "baseUrl": "./src",
    },
    "include": [
        "src/**/*.ts",
        "src/**/*.tsx",
        "src/**/*.js",
        "src/**/*.jsx",
    ]
}